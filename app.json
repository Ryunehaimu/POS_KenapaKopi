{
  "expo": {
    "name": "POS Kenapa Kopi",
    "scheme": "POS Kenapa Kopi",
    "slug": "pos-kenapa-kopi",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon-custom.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-custom.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-custom.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kenapakopi.pos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera for attendance."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2cb908d4-18e7-47d8-983c-8880a67f15e0"
      }
    }
  }
}
